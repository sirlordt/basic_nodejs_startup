  "preLaunchTask": "npm: copy:resource tsc: build - tsconfig.json",
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/index.ts",
      "preLaunchTask": "build",
      "outputCapture": "std",
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ]
    }
  ]
}
